{
  "name": "ns-accounts",
  "version": "0.0.1",
  "description": "My accounts application.",
  "main": "electron/electron.js",
  "homepage": "./",
  "scripts": {
    "dev": "webpack-dev-server --config ./config/webpack.dev.js",
    "build": "webpack --config ./config/webpack.prod.js",
    "start": "concurrently \"cross-env BROWSER=none npm run dev\" \"wait-on http://localhost:9000 && electron . --inspect --enable-logging\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NatasXWY/ns-accounts.git"
  },
  "keywords": [
    "electron",
    "account",
    "reactjs"
  ],
  "author": "Natas@X",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NatasXWY/ns-accounts/issues"
  },
  "dependencies": {
    "@ant-design/dark-theme": "^1.0.3",
    "@material-ui/core": "^4.8.0",
    "@material-ui/icons": "^4.5.1",
    "antd": "^3.26.3",
    "color": "^3.1.2",
    "electron-is-dev": "^1.1.0",
    "fbemitter": "^2.1.1",
    "history": "^4.10.1",
    "moment": "^2.24.0",
    "promise": "^8.0.3",
    "promise.prototype.finally": "^3.1.2",
    "prop-types": "^15.7.2",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-arrow-functions": "^7.7.4",
    "@babel/plugin-transform-regenerator": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-stage-2": "^7.0.0",
    "@babel/runtime": "^7.7.7",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.0",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.2",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.0",
    "electron": "^9.4.0",
    "file-loader": "^5.0.2",
    "hoek": "^6.1.3",
    "html-webpack-plugin": "^3.2.0",
    "less": "^2.7.2",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router": "^5.1.2",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^3.0.0",
    "wait-on": "^3.3.0",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false,
          "targets": {
            "browsers": [
              "> 1%",
              "last 2 versions",
              "not ie <=8"
            ]
          }
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd",
          "style": true
        }
      ],
      "@babel/transform-runtime",
      "@babel/plugin-transform-regenerator",
      "@babel/plugin-transform-arrow-functions",
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
